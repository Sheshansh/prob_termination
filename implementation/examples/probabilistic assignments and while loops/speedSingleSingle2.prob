var x,n,y,m,break;
break:= [-1,1]+0;
if n<0 or m <0 then skip else
  x:= [-1,1]+0;
  y:= [-1,1]+0;
  while break>=0 and break<=0
    do
if prob(0.5) then
		if x<n then x:= [-1,1]+x+1;y:= [-1,1]+y+1 
		else
			if y<m then x:= [-1,1]+x+1;y:= [-1,1]+y+1 else break:= [-1,1]+1 fi
		fi
    else skip fi
od
fi