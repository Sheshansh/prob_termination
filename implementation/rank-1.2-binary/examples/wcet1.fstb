model wcet1{

//parameters i__o, j__o, n__o, tmp_1__o;

  var i, j, n, tmp_1, i__o, j__o, n__o, tmp_1__o;

  states stop, start, cut, ____start;

  transition t_43 :={
   from    := start ;
   to      := stop ;
