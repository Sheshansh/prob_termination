model main{


  var c1,c2,n1,n2,x,y,z;

  states state_1, state_2, state_8, state_8_1, state_14;

  transition t_50 :={
   from    := state_1 ;
   to      := state_2 ;
   guard   := n2<=2y;
   action  := c1' = 0;
  };


  transition t_52 :={
   from    := state_1 ;
   to      := state_2 ;
   guard   := n2>2y && n2<=y+z;
   action  := c1' = 0;
  };


  transition t_54__tsplit0 :={
   from    := state_1 ;
   to      := state_2 ;
   guard   := n2>y+z && n2>2y && n1<0 || n2<0 || x<0 || y<0 || z<0;
   action  := c1' = 0;
  };



  transition t_57 :={
   from    := state_1 ;
   to      := state_8_1 ;
   guard   := n2>y+z && n2>2y && x>=0 && y>=0 && z>=0 && n1>=0 && n2>=0;
   action  := c1' = 0;
  };

  transition t_57 :={
   from    := state_8_1 ;
   to      := state_2 ;
   guard   := n1<1;
   action  := n1' = n1 - 1;
  };


  transition t_64 :={
   from    := state_1 ;
   to      := state_14 ;
   guard   := n1>=1 && n2>y+z && n2>2y && x>=0 && y>=0 && z>=0;
   action  := c1' = c1+1,c2' = 0,n1' = n1 - 1;
  };


  transition t_55 :={
   from    := state_8 ;
   to      := state_2 ;
   guard   := n1<1;
   action  := n1' = n1 - 1;
  };


  transition t_61 :={
   from    := state_8 ;
   to      := state_8 ;
   guard   := n1>=1 && n2<=0;
   action  := c1' =  1  + c1,c2' = 0,n1' = n1 - 1,n2' =  y  + z,z' =  y  + z;
  };


  transition t_62 :={
   from    := state_8 ;
   to      := state_14 ;
   guard   := n1>=1 && n2>0;
   action  := c1' =  1  + c1,c2' = 1,n1' = n1 - 1,n2' = n2 - 1,z' = y;
  };


  transition t_59 :={
   from    := state_14 ;
   to      := state_8 ;
   guard   := n2<=0;
   action  := n2' =  y  + z,z' =  y  + z;
  };


  transition t_60 :={
   from    := state_14 ;
   to      := state_14 ;
   guard   := n2>0;
   action  := c2' =  1  + c2,n2' = n2 - 1,z' = y;
  };


}

